{
      "name": "todoapp",
      "private": true,
      "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "format": "prettier write \"**/*.{ts,tsx,md}\"",
        "checktypes": "turbo run checktypes",
        "db:generate":"cd packages/db && bunx prisma generate && cd ../..",
        "start:web":"cd apps/web && bun run start",
        "start:backend":"cd apps/backend && bun run index.ts",
        "start:websocket":"cd apps/websocket && bun run index.ts"
      },
      "devDependencies": {
        "prettier": "^3.7.4",
        "turbo": "^2.7.1",
        "typescript": "5.9.2"
      },
      "engines": {
        "node": ">=18"
      },
      "packageManager": "bun@1.3.5",
      "workspaces": [
        "apps/*",
        "packages/*"
      ]
    }